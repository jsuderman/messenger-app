g(response.data);\n                setRooms(response.data);\n            });\n    }, []);\n\n    useEffect(() => {\n        const pusher = new Pusher('e49696d1456791ae2abf', {\n            cluster: 'us3'\n        });\n\n        const channel = pusher.subscribe('rooms');\n        channel.bind('inserted', function (data) {\n            // alert(JSON.stringify(data));\n            setRooms([...rooms, data])\n        });\n\n        return () => {\n            channel.unbind_all();\n            channel.unsubscribe();\n        }\n\n    }, [rooms]);\n\n    // console.log(rooms);\n\n    return (\n        <div className=\"sidebar\">\n\n            <div className=\"sidebar__header\">\n                <Avatar\n                    src={user?.photoURL} />\n\n                <div className=\"sidebar__headerRight\">\n                    <IconButton>\n                        <DonutSmallIcon />\n                    </IconButton>\n\n                    <IconButton>\n                        <MessageIcon />\n                    </IconButton>\n\n                    <IconButton>\n                        <MoreHorizIcon />\n                    </IconButton>\n                </div>\n            </div>\n\n            <div className=\"sidebar__search\">\n                <div className=\"sidebar__searchContainer\">\n                    <SearchIcon />\n                    <input placeholder=\"search or start new chat\" type=\"text\" />\n                </div>\n            </div>\n\n            <div className=\"sidebar__chats\">\n                <SidebarChat addNewChat />\n                {rooms.map(room => (\n                    <SidebarChat key={room._id} id={room._id}\n                        name={room.name}\n                    />\n                ))}\n\n            </div>\n\n        </div>\n    )\n}\n\nexport default Sidebar\n","/Users/justinsuderman/Desktop/Projects/messenger-app/client/messenger-app/src/components/Chat/Chat.js",["50","51","52","53","54","55"],"/Users/justinsuderman/Deskt